module Fib;

type <java> "java.lang.Integer" from "rt.jar" as int;

global protocol Adder(role C, role S)
{
	HELLO(u:int) from C to S;
	SUM(v:int) from S to C;
	//RES(f:int) from C to S;
	choice at C {
		RES(a:int) from C to S;
	} or {
		BYE(b:int) from C to S;
	}
}                                           
