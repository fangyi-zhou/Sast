module FibnoAss;

type <java> "java.lang.Integer" from "rt.jar" as int;


global protocol Adder(role C, role S)
{
	HELLO(int) from C to S;
	HELLO(int) from S to C;
	
	rec Loop {
		HELLO(int) from C to S;
		choice at C
		{
			ADD(int) from C to S; 
			RES(int) from S to C;
			continue Loop; 
		}
		or
		{
			BYE(int) from C to S;
			BYE(int) from S to C;
		}
}
}
